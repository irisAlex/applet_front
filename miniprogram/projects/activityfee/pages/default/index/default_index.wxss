  .assets {
    width: 100%;
    background-color: #ffffff;
    padding: 20rpx;
    border-radius: 20rpx;
    box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
  }
  
  .assets-header {
    font-size: 32rpx;
    color: #333;
    margin-bottom: 20rpx;
  }
  
  .assets-item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 0;
    border-bottom: 1rpx solid #f0f0f0;
  }
  
  .assets-item:last-child {
    border-bottom: none;
  }
  
  .assets-item-text {
    font-size: 28rpx;
    color: #333;
  }
  
  .assets-item-change {
    font-size: 24rpx;
    color: #4caf50;
  }



  .repair-list {
    margin-top: 20rpx;
  }
  
  .repair-item {
    /* border: 1px solid #e0e0e0; */
    border-radius: 10rpx;
    padding: 20rpx;
    margin-bottom: 20rpx;
  }
  
  .repair-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-bottom: 10rpx;
    border-bottom: 1px solid #e0e0e0;
  }
  
  .repair-title {
    font-size: 32rpx;
    font-weight: bold;
  }
  
  .repair-status {
    font-size: 28rpx;
    color: #ff9500;
  }
  
  .repair-body {
    padding-top: 10rpx;
  }
  
  .repair-type, .repair-desc, .repair-time {
    display: block;
    font-size: 26rpx;
    color: #666;
    margin-top: 5rpx;
  }

  .footer {
    width: 100%;
    display: flex;
    justify-content: space-around;
    padding: 20rpx 0;
    background-color: #ffffff;
    box-shadow: 0 -2rpx 8rpx rgba(0, 0, 0, 0.1);
  }
  
  .footer-item {
    text-align: center;
    color: #888;
    font-size: 24rpx;
    position: relative;
  }
  
  .footer-item.active {
    color: #007bff;
  }
  
  .footer-item::before {
    content: '';
    display: block;
    width: 50rpx;
    height: 50rpx;
    margin: 0 auto 10rpx;
    background-size: contain;
    background-repeat: no-repeat;
  }
  
  .footer-item.home::before {
    background-image: url('data:image/svg+xml;base64,');
  }
  
  .footer-item.data::before {
    background-image: url('data:image/svg+xml;base64,');
  }
  
  .footer-item.community::before {
    background-image: url('data:image/svg+xml;base64,');
  }
  