/* styles.wxss */
@import "../../../../../style/public/comm_box_list.wxss";
@import "../../../style/skin.wxss";
page {
	background-color: #ffffff
}
.container {
    flex-direction: column;
    align-items: center;
    padding: 50rpx;
    margin-top: 150rpx;
  }
  
  .header {
    width: 100%;
    padding: 40rpx 20rpx;
    box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
    border-radius: 20rpx;
    height: 200px;
    background-image: url('data:image/png;base64,');
    background-size: cover;
    background-position: center;
  }
  
  .header-text {
    font-size: 36rpx;
    color: #333;
    margin-bottom: 20rpx;
  }
  
  .header-subtext {
    font-size: 28rpx;
    color: #888;
  }
  
  .header-button {
    display: inline-block;
    padding: 10rpx 20rpx;
    margin-top: 20rpx;
    border-radius: 20rpx;
    background-color: #007bff;
    color: #ffffff;
    text-align: center;
  }
  
  .stats {
    width: 100%;
    display: flex;
    justify-content: space-between;
    margin: 20rpx 0;
  }
  
  .stats-item {
    flex: 1;
    margin: 0 10rpx;
    padding: 20rpx;
    border-radius: 20rpx;
    background-color: #ffffff;
    text-align: center;
  }
  
  .stats-item.blue {
    background-size: cover;
    background-position: center;
    background-image: url('data:image/jpeg;base64,');
  }
  
  .stats-item.pink {
    background-image: url('data:image/jpeg;base64,');
    background-size: cover;
    background-position: center;
  }
  
  .stats-item.green {
    background-image: url('data:image/jpeg;base64,');
    background-size: cover;
    background-position: center;
  }
  
  .stats-item-text {
    font-size: 28rpx;
    color: #ffffff;
    margin-bottom: 10rpx;
  }
  
  .stats-item-value {
    font-size: 36rpx;
    color: #ffffff;
  }
  
  .stats-item-change {
    font-size: 24rpx;
    color: #ffffff;
    margin-top: 10rpx;
  }
  
  .assets {
    width: 100%;
    background-color: #ffffff;
    padding: 20rpx;
    border-radius: 20rpx;
    box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
  
  }


  .lable {
    display: flex;
    justify-content: space-between;
    margin: 20rpx 0;
    width: 100%;
    background-color: #F7FAFF;
    padding: 40rpx;
    border-radius: 20rpx;
    height: 200rpx;
    box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
    padding-bottom: 30rpx;
  }

  .lable-image{
    display: flex;
    width: 80rpx;
    height: 80rpx;
    background-size: cover;
    background-position: center;
    mix-blend-mode: multiply;
    background-image:url('data:image/png;base64,');
  }

  .lable-image1{
    display: flex;
    width: 80rpx;
    height: 80rpx;
    mix-blend-mode: multiply;
    background-size: cover;
    background-position: center;
    background-image:url('data:image/jpeg;base64,');
  }

  .lable-image2{
    display: flex;
    width: 80rpx;
    height: 80rpx;
    background-size: cover;
    background-position: center;
    mix-blend-mode: multiply;
    background-image:url('data:image/jpeg;base64,');
  }

  .lable-image3{
    display: flex;
    width: 80rpx;
    height: 80rpx;
    background-size: cover;
    background-position: center;
    mix-blend-mode: multiply;
    background-image:url('data:image/jpeg;base64,');
  }

  .lable-text{
    margin: 20rpx 0;
    display: flex;
    text-align: center;
    color: #888;
    font-size: 24rpx;
    position: relative;
  }

